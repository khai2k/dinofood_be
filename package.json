    "@hapi/vision": "^5.5.4",
    "archiver": "^5.2.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "config": "^3.3.0",
    "cron": "^1.8.2",
    "csv": "^5.3.2",
    "firebase-admin": "^9.3.0",
    "googleapis": "^61.0.0",
    "hapi18-swagger": "^1.0.1",
    "html-minifier": "^4.0.0",
