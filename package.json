  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "rm -rf .husky&&husky install&&husky add .husky/pre-commit \"npm run precommit\"",
    "precommit": "lint-staged",
    "bower:install": "bower install",
    "start": "nodemon --exec npm run watch",
    "watch": "npm-run-all --serial eslint dev",
    "dev": "cross-env NODE_ENV=development node --inspect=0.0.0.0:9226 --trace-warnings index.js",
    "staging": "cross-env NODE_ENV=staging node index.js",
    "production": "cross-env NODE_ENV=production node index.js",
    "eslint": "eslint .",
    "test": "test"
  },
  "dependencies": {
